    "class Poke:\n",
    "    def __init__(self, name, ability, base_exp, sprite_url):\n",
    "        self.name = name\n",
    "        self.ability = ability\n",
    "        self.base_exp = base_exp\n",
    "        self.sprite_url = sprite_url\n",
    "    def __repr__(self):\n",
    "        return \"\\n\".join(f\"{self.__dict__}\"[1:-1].split(\", \"))"
   ]
  },
